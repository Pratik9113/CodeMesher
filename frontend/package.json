{
  "name": "mycursor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Pratik9113/CodeMesher.git"
  },
  "scripts": {
    "dev:ui": "vite",
    "dev:electron": "wait-on http://localhost:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "dev": "concurrently -k \"npm:dev:ui\" \"npm:dev:electron\"",
    "build:ui": "tsc -b && vite build",
    "build:electron": "electron-builder --publish never",
    "build": "npm run build:ui && npm run build:electron",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@octokit/rest": "^22.0.0",
    "axios": "^1.12.2",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.544.0",
    "marked": "^16.4.0",
    "mermaid": "^11.12.0",
    "monaco-editor": "^0.53.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.4",
    "react-zoom-pan-pinch": "^3.7.0",
    "remark-gfm": "^4.0.1",
    "three": "^0.180.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/highlight.js": "^9.12.4",
    "@types/marked": "^5.0.2",
    "@types/mermaid": "^9.1.0",
    "@types/node": "^24.7.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@types/three": "^0.180.0",
    "@vitejs/plugin-react": "^5.0.3",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7",
    "wait-on": "^9.0.1"
  },
  "main": "electron/main.cjs",
  "build": {
    "appId": "com.example.mycursor",
    "productName": "MyCursor",
    "files": [
      "dist/**",
      "electron/**",
      "package.json"
    ],
    "directories": {
      "buildResources": "build"
    },
    "win": {
      "icon": "build/icon.ico"
    }
  }
}
